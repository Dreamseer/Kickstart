{
  "name": "kickstart",
  "version": "4.2.0",
  "description": "This is my Kickstart toolset, serving as a boilerplate for new projects written in PHP, CSS and JavaScript.",
  "private": true,
  "keywords": [
    "kickstart",
    "boilerplate",
    "php",
    "html",
    "css",
    "javascript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dreamseer/kickstart.git"
  },
  "author": {
    "name": "Marc GÃ¶rtz",
    "email": "git@marcgoertz.de",
    "url": "https://marcgoertz.de/"
  },
  "engines": {
    "node": ">=4.4.x"
  },
  "dependencies": {
    "jquery": "^3.6.0",
    "pepjs": "^0.5.3"
  },
  "devDependencies": {
    "cssnano": "^5.0.2",
    "eslint": "^7.25.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-unicorn": "^31.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-modernizr-build": "^0.0.6",
    "gulp-postcss": "^9.0.0",
    "gulp-requirejs-optimize": "^1.3.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "modernizr": "^3.11.7",
    "postcss": "^8.2.13",
    "postcss-assets": "^5.0.0",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-reporter": "^7.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
  },
  "scripts": {
    "build": "gulp",
    "test": "gulp lint"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:unicorn/recommended",
      "plugin:compat/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "script"
    },
    "env": {
      "amd": true,
      "browser": true,
      "jquery": true
    },
    "rules": {
      "unicorn/prefer-module": "off"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "indentation": "tab"
    }
  },
  "browserslist": [
    "last 2 versions"
  ]
}
